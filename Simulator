        				sr=(n%2)+sr;
        			encode=encode+sr;
        		}
        		writer.write(encode+"\r\n");
        		
        	}
        	else if(command.equals("mod"))
        	{
        		encode=encode+opcode[4];
        		String s1=st.nextToken();
        		String s2=st.nextToken();
        		if(s2.charAt(0)=='r')
        		{
        			encode=encode+'0'+regcode[reg.charAt(1)-48]+regcode[s1.charAt(1)-48]+"00000000000000"+regcode[s2.charAt(1)-48];
        		}
        		else
        		{
        			encode=encode+'1'+regcode[reg.charAt(1)-48]+regcode[s1.charAt(1)-48];
        			String sr="";
        			int n=Integer.parseInt(s2);
        			for(int k=1;k<=18;n=n/2,k++)
        				sr=(n%2)+sr;
        			encode=encode+sr;
        		}
        		writer.write(encode+"\r\n");
        		
        	}
        	else if(command.equals("cmp"))
        	{
        		encode=encode+opcode[5];
        		String s1=st.nextToken();
        		if(s1.charAt(0)=='r')
        		{
        			encode=encode+"0XXXX"+regcode[reg.charAt(1)-48]+"00000000000000"+regcode[s1.charAt(1)-48];
        		}
        		else
        		{
        			encode=encode+"1XXXX"+regcode[reg.charAt(1)-48];
        			String sr="";
        			int n=Integer.parseInt(s1);
        			for(int k=1;k<=18;n=n/2,k++)
        				sr=(n%2)+sr;
        			encode=encode+sr;
        		}
        		writer.write(encode+"\r\n");
        		
        	}
        	else if(command.equals("and"))
        	{
        		encode=encode+opcode[6];
        		String s1=st.nextToken();
        		String s2=st.nextToken();
        		if(s2.charAt(0)=='r')
        		{
        			encode=encode+'0'+regcode[reg.charAt(1)-48]+regcode[s1.charAt(1)-48]+"00000000000000"+regcode[s2.charAt(1)-48];
        		}
        		else
