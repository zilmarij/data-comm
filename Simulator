        				sr=(n%2)+sr;
        			encode=encode+sr;
        		}
        		writer.write(encode+"\r\n");
        		
        	}
        	else if(command.equals("or"))
        	{
        		encode=encode+opcode[7];
        		String s1=st.nextToken();
        		String s2=st.nextToken();
        		if(s2.charAt(0)=='r')
        		{
        			encode=encode+'0'+regcode[reg.charAt(1)-48]+regcode[s1.charAt(1)-48]+"00000000000000"+regcode[s2.charAt(1)-48];
        		}
        		else
        		{
        			encode=encode+'1'+regcode[reg.charAt(1)-48]+regcode[s1.charAt(1)-48];
        			String sr="";
        			int n=Integer.parseInt(s2);
        			for(int k=1;k<=18;n=n/2,k++)
        				sr=(n%2)+sr;
        			encode=encode+sr;
        		}
        		writer.write(encode+"\r\n");
        		
        	}
        	else if(command.equals("not"))
        	{
        		encode=encode+opcode[8];
        		String s1=st.nextToken();
        		if(s.charAt(0)=='r')
        		{
        			encode=encode+'0'+regcode[reg.charAt(1)-48]+"XXXX00000000000000"+regcode[s1.charAt(1)-48];
        		}
        		writer.write(encode+"\r\n");
        		
        	}
        	else if(command.equals("mov"))
        	{
        		encode=encode+opcode[9];
        		String s1=st.nextToken();
        		if(s1.charAt(0)=='r')
        		{
        			encode=encode+'0'+regcode[reg.charAt(1)-48]+"XXXX00000000000000"+regcode[s1.charAt(1)-48];
        		}
        		else
        		{
        			encode=encode+'1'+regcode[reg.charAt(1)-48]+"XXXX";
        			String sr="";
        			int n=Integer.parseInt(s1);
        			for(int k=1;k<=18;n=n/2,k++)
        				sr=(n%2)+sr;
        			encode=encode+sr;
        		}
        		writer.write(encode+"\r\n");
        		
        	}
        	else if(command.equals("lsl"))
        	{
        		encode=encode+opcode[10];
        		String s1=st.nextToken();
        		String s2=st.nextToken();
        		if(s2.charAt(0)=='r')
        		{
        			encode=encode+'0'+regcode[reg.charAt(1)-48]+regcode[s1.charAt(1)-48]+"00000000000000"+regcode[s2.charAt(1)-48];
        		}
        		else
        		{
        			encode=encode+'1'+regcode[reg.charAt(1)-48]+regcode[s1.charAt(1)-48];
        			String sr="";
        			int n=Integer.parseInt(s2);
        			for(int k=1;k<=18;n=n/2,k++)
        				sr=(n%2)+sr;
        			encode=encode+sr;
        		}
        		writer.write(encode+"\r\n");
        		
        	}
        	else if(command.equals("lsr"))
        	{
        		encode=encode+opcode[11];
        		String s1=st.nextToken();
        		String s2=st.nextToken();
        		if(s2.charAt(0)=='r')
        		{
        			encode=encode+'0'+regcode[reg.charAt(1)-48]+regcode[s1.charAt(1)-48]+"00000000000000"+regcode[s2.charAt(1)-48];
        		}
        		else
        		{
        			encode=encode+'1'+regcode[reg.charAt(1)-48]+regcode[s1.charAt(1)-48];
        			String sr="";
        			int n=Integer.parseInt(s2);
        			for(int k=1;k<=18;n=n/2,k++)
        				sr=(n%2)+sr;
        			encode=encode+sr;
        		}
        		writer.write(encode+"\r\n");
        		
        	}
        	else if(command.equals("asr"))
        	{
        		encode=encode+opcode[12];
        		String s1=st.nextToken();
        		String s2=st.nextToken();
        		if(s2.charAt(0)=='r')
        		{
        			encode=encode+'0'+regcode[reg.charAt(1)-48]+regcode[s1.charAt(1)-48]+"00000000000000"+regcode[s2.charAt(1)-48];
        		}
        		else
        		{
        			encode=encode+'0'+regcode[reg.charAt(1)-48]+regcode[s1.charAt(1)-48];
        			String sr="";
        			int n=Integer.parseInt(s2);
        			for(int k=1;k<=18;n=n/2,k++)
        				sr=(n%2)+sr;
        			encode=encode+sr;
        		}
        		writer.write(encode+"\r\n");
        		
        	}
        	else if(command.equals("nop"))
        	{
        		encode=encode+opcode[13];
        		encode=encode+"XXXXXXXXXXXXXXXXXXXXXXXXXXX";
        		writer.write(encode+"\r\n");
        		
        	}
        	else if(command.equals("ld"))
        	{
        		encode=encode+opcode[14];
        		String s1=st.nextToken();
        		if(s1.charAt(0)!='#')
        		{
        			encode=encode+'0'+regcode[reg.charAt(1)-48]+"XXXX";
        			String sr="";
        			int n=Integer.parseInt(memory[Integer.parseInt(s1)]);
        			for(int k=1;k<=18;n=n/2,k++)
        				sr=(n%2)+sr;
        			encode=encode+sr;
        		}
        		else
        		{
        			encode=encode+'0'+regcode[reg.charAt(1)-48]+"XXXX";
        			String sr="";
        			int n=Integer.parseInt(s1.substring(1));
        			for(int k=1;k<=18;n=n/2,k++)
        				sr=(n%2)+sr;
        			encode=encode+sr;
        		}
        		writer.write(encode+"\r\n");
        		
        	}
        	else if(command.equals("st"))
        	{
        		encode=encode+opcode[15];
        		String s1=st.nextToken();
        		if(s1.charAt(0)!='#')
        		{
        			encode=encode+'0'+regcode[reg.charAt(1)-48]+"XXXX";
        			String sr="";
        			int n=Integer.parseInt(memory[Integer.parseInt(s1)]);
        			for(int k=1;k<=18;n=n/2,k++)
        				sr=(n%2)+sr;
        			encode=encode+sr;
        		}
        		else
        		{
        			encode=encode+'1'+regcode[reg.charAt(1)-48]+"XXXX";
        			String sr="";
        			int n=Integer.parseInt(s1.substring(1));
        			for(int k=1;k<=18;n=n/2,k++)
        				sr=(n%2)+sr;
        			encode=encode+sr;
        		}
        		writer.write(encode+"\r\n");
        	}
        	else if(command.equals("beg"))
        	{
        		encode=encode+opcode[16];
        		int j;
        		for(j=0;j<=i;j++)
        			if(reg.equals(sarray[j]))
        				break;
        		String sr="";
        		int n=j;
        			for(int k=1;k<=27;n=n/2,k++)
        				sr=(n%2)+sr;
        			encode=encode+sr;
        				
        			writer.write(encode+"\r\n");       		
        		
        	}
        	else if(command.equals("bgt"))
        	{
        		encode=encode+opcode[17];
        		int j;
        		for(j=0;j<=i;j++)
        			if(reg.equals(sarray[j]))
        				break;
        		String sr="";
        		int n=j;
        		for(int k=1;k<=27;n=n/2,k++)
        			sr=(n%2)+sr;
        			encode=encode+sr;
        		writer.write(encode+"\r\n");
        	}
        	else if(command.equals("b"))
        	{
        		encode=encode+opcode[18];
        		int j;
        		for(j=0;j<=i;j++)
        			if(reg.equals(sarray[j]))
        				break;
        		String sr="";
        		int n=j;
        		for(int k=1;k<=27;n=n/2,k++)
        			sr=(n%2)+sr;
        		encode=encode+sr; 
        		writer.write(encode+"\r\n");   		
        	}
        	else if(command.equals("call"))
        	{
        		encode=encode+opcode[19];
        		int j;
        		for(j=0;j<=i;j++)
        			if(reg.equals("function"+" "+sarray[j]))
        				break;
        		String sr="";
        		int n=j;
        		for(int k=1;k<=27;n=n/2,k++)
        			sr=(n%2)+sr;
        		encode=encode+sr;  
        		writer.write(encode+"\r\n");  	   		
        	}
        	else if(command.equals("ret"))
        	{
        		encode=encode+opcode[20];
        	}	
        }
        writer.close();
	}
	public static void main(String args[])throws IOException
	{
		DataInputStream x=new DataInputStream(System.in);
		Simulator ob=new Simulator();
		ob.mains();
	}
		
}